DEFINITION MODULE MsgBox;

TYPE
  But_Napis = ARRAY[0..15] OF CHAR;

PROCEDURE Info( Text : ARRAY OF CHAR ) : BOOLEAN; (* T/N *)

PROCEDURE AdvInfo(     Text   : ARRAY OF CHAR;
                       IleNap : CARDINAL;
